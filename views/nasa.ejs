<!-- views/nasa.ejs -->

  <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css">
  <link rel="stylesheet" href="/style.css">

 <div class="grey-out youtube"> 
    <div class="box nasa">
      <div class="nasa-box">
          <a href="/back"><button type="button" name="back">Back</button></a>
          <% if (picture) { %>
            <h3 id="apod_title"><%= picture.title %></h3>
            <div class="nasa-img">
              <img id="apod_img_id" src="<%= picture.url %>">
           <!--  <iframe id="apod_vid_id" type="text/html" width="640" height="385" frameborder="0"></iframe> -->
           </div>
            <p id="photo-date"><%= picture.date %></p>
            <p id="apod_explanation"><%= picture.explanation %></p>
            <form><button type="button" name="picture" value="<%= picture %>" formmethod="post" formaction=" ">Save</button></form>
          <% } %>
      </div>
  </div>
</div>
